<script>
function getByClass(oParent,sClass)
{
	var aEle = oParent.getElementsByTagName('*');
	var aResult = [];
	var re=new RegExp('\\b'+sClass+'\\b', 'i');
	
	for(var i=0; i<aEle.length;i++)
	{
		if(aEle[i].className.search(re)!=-1)
		{
			aResult.push(aEle[i]);
		}
	}
	return aResult;
}
window.onload = function(){
	var oLink = document.getElementById('__lost_child_support_company');
	
	oLink.innerHTML = '<a title="智能社 - 公益 - 为失踪孩子奉献我们自己的一份微薄之力，让 宝贝回家 " href="http://www.zhinengshe.com/" target="_blank" style="margin-top:4px !important;">智能社公益</a>'
	
	
	//返回首页链接
	var returnIndex = getByClass(document,'back_home')[0];
	returnIndex.href = 'http://www.zhinengshe.com/';
}
</script>
